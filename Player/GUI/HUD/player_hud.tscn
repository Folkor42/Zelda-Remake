[sub_resource type="GDScript" id="GDScript_gx1g6"]
script/source = "extends Sprite2D

func _ready() -> void:
	if PlayerManager.sword == \"Wooden Sword\":
		region_rect = Rect2(555, 137, 7, 16)
	elif PlayerManager.sword == \"White Sword\":
		region_rect = Rect2(564, 137, 7, 16)
	elif PlayerManager.sword == \"Magic Sword\":
		region_rect = Rect2(573, 137, 8, 16)
	else:
		region_rect = Rect2(495, 145, 7, 16)
		
func update_b () -> void:
	if PlayerManager.sword == \"Wooden Sword\":
		region_rect = Rect2(555, 137, 7, 16)
	elif PlayerManager.sword == \"White Sword\":
		region_rect = Rect2(564, 137, 7, 16)
	elif PlayerManager.sword == \"Magic Sword\":
		region_rect = Rect2(573, 137, 8, 16)
	else:
		region_rect = Rect2(495, 145, 7, 16)
"

[sub_resource type="GDScript" id="GDScript_07qm7"]
script/source = "extends Sprite2D

func _ready() -> void:
	region_rect = Rect2(495, 145, 7, 16)
		
func update (rect : Rect2 ) -> void:
	if rect:
		region_rect = rect
	else:
		region_rect = Rect2(495, 145, 7, 16)
"

[sub_resource type="GDScript" id="GDScript_v8b4v"]
script/source = "extends Label

func _process(_delta: float) -> void:
